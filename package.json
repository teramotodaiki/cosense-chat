{
  "name": "cosense-chat",
  "version": "0.1.0",
  "private": true,
  "type": "commonjs",
  "scripts": {
    "pretest": "playwright install --with-deps chromium",
    "build": "vite build",
    "test": "vitest run && VITE_OPENAI_API_KEY=test vite build && playwright test __tests__/script.test.js",
    "lint": "eslint . --ext .js",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "format": "biome format --write .",
    "format:check": "biome check ."
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.2",
    "@playwright/test": "^1.55.0",
    "eslint": "^8.57.0",
    "typescript": "^5.5.4",
    "vite": "^5.4.0",
    "vitest": "^1.6.0"
  }
}
